{
  "name": "@react-above/modal-theme-default",
  "description": "A default theme for @react-above/modal",
  "version": "0.0.0-set-by-ci",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./dist/styles.css": {
      "require": "./dist/styles.css",
      "import": "./dist/styles.css",
      "default": "./dist/styles.css"
    }    
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "license": "MIT",
  "repository": "git@github.com:react-above/modal-theme-default.git",
  "author": "Evgeny Zakharov <to@risen.dev>",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && rollup -c",
    "test": "jest",
    "lint": "eslint --ext .ts,.tsx src"
  },
  "devDependencies": {
    "@eslint-kit/eslint-config-base": "^4.0.0",
    "@eslint-kit/eslint-config-node": "^2.0.0",
    "@eslint-kit/eslint-config-patch": "^1.0.0",
    "@eslint-kit/eslint-config-prettier": "^3.0.0",
    "@eslint-kit/eslint-config-typescript": "^5.2.0",
    "@react-above/modal": "^0.3.3",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^26.0.14",
    "@types/react": "^16.9.23",
    "@typescript-eslint/parser": "4.15.0",
    "esbuild": "^0.14.24",
    "eslint": "7.32.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "jest": "^28.0.3",
    "prettier": "2.2.1",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.69.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.51.0",
    "ts-jest": "^26.4.0",
    "typescript": "4.6.3",
    "vite": "^2.9.6",
    "vite-plugin-eslint": "^1.6.0",
    "vite-tsconfig-paths": "^3.4.1"
  },
  "peerDependencies": {
    "@react-above/modal": "*",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "dependencies": {
    "clsx": "^1.1.1"
  }
